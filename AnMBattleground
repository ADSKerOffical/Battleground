   if v.Name == "Technique Pull" then
       v.Parent = LP.Character
   end
end
      		while wait(0.2) do
game:GetService("Players").LocalPlayer.Character:FindFirstChild("Technique Pull").ao.Event:FireServer()
end
  	end
})

