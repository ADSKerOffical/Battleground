local Tab = Window:MakeTab({
	Name = "More",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
 Name = "Anti purple shot",
 Callback = function()
        local player = game:GetService("Players").LocalPlayer
while task.wait() do
local parts = workspace:GetPartBoundsInRadius(player.Character:WaitForChild("HumanoidRootPart").Position, 10)
for _, part in ipairs(parts) do
part.CanTouch = fasle
end
end
   end    
})

Tab:AddToggle({
	Name = "Anti slow",
	Default = false,
	Callback = function(Value)
	EEr = Value
		while EEr do
  wait()
local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

local initialSpeed = 16
local currentSpeed = Character.Humanoid.WalkSpeed

if currentSpeed >= 0 and currentSpeed <= 10 then
    Character.Humanoid.WalkSpeed = initialSpeed
else
    Character.Humanoid.WalkSpeed = currentSpeed
end
 end
	end    
})

Tab:AddToggle({
	Name = "Anti inventory lock",
	Default = false,
	Callback = function(Value)
	Sw = Value
		while Sw do
		local StarterGui = game:GetService("StarterGui")
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)

local sgui = game:GetService("StarterGui")
sgui:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, true)
		wait()
		end
	end    
})

Tab:AddDropdown({
	Name = "Input Character",
	Default = "1",
	Options = {"Gojo", "Sukuma", "Toji", "Saitama", "Luffy", "Hakari", "Sakuya", "Kenjaku"},
	Callback = function(Value)
		if Value == "Gojo" then
		local args = {
    [1] = "Gojo Young"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
		end
		if Value == "Sukuma" then
		local args = {
    [1] = "Yuji / Sukuna"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
		end
		if Value == "Toji" then
		local args = {
    [1] = "Toji"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
		end
		if Value == "Saitama" then
		local args = {
    [1] = "Saitama"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
		end
		if Value == "Luffy" then
		local args = {
    [1] = "Luffy"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
		end
if Value == "Hakari" then
local args = {
    [1] = "Hakari"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
end
if Value == "Sakuya" then
local args = {
    [1] = "Sakuya"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
end
if Value == "Kenjaku" then
local args = {
    [1] = "Kenjaku"
}

game:GetService("ReplicatedStorage"):WaitForChild("Picked"):FireServer(unpack(args))
end
	end    
})

Tab:AddToggle({
	Name = "Godmode",
	Default = false,
	Callback = function(Value)
	God = Value
		while God do
		wait(0.1)
		if game:GetService("Players").LocalPlayer.Character.Humanoid.Health < 30 then
		local args = {
    [1] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("Transformation"):FireServer(unpack(args))
end
		end
	end    
})

Tab:AddButton({
 Name = "Anti Void",
 Callback = function()
        local part = Instance.new("Part")
part.Name = "AntiFallPart"
part.Size = Vector3.new(50000, 5, 50000)
part.Transparency = 1
part.Anchored = true
part.Position = Vector3.new(8, -47, -92)
part.Parent = game.Workspace
   end    
})

Tab:AddButton({
 Name = "Anti jump block",
 Callback = function()
        while true do
  wait()
local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

local initialJump = 50
local currentJump = Character.Humanoid.JumpPower

if currentJump >= 0 and currentJump <= 10 then
    Character.Humanoid.JumpPower = initialJump
else
    Character.Humanoid.JumpPower = currentJump
end
 end
   end    
})
